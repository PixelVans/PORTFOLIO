{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-console": "warn",
    "react/no-unescaped-entities": "off",  // Disables the rule for unescaped entities
    "quotes": [
      "off",  // Disables the quotes rule
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ]
  }
}
